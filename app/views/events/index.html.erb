    <!-- Header -->
    <header>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <img class="img-responsive" src="assets/images/telcoz-logo_360.png" alt="">
                    <div class="intro-text">
                        <span class="name">Telcoz</span>
                        
                        <span class="skills">Engaging with talent on leave</span>
                    </div>
                </div>
            </div>
        </div>
    </header>


<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table class="table table-bordered  table-hover ">
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Start</th>
      <th>End</th>
      <th>Description</th>
      <th>Address</th>
      <th>Additional Information</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.eventname, employee_event_path %></td>
        <td><%= event.eventdate %></td>
        <td><%= event.eventstarttime.strftime(' %H:%M %Z')  %></td>
        <td><%= event.eventendtime.strftime(' %H:%M %Z') %></td>
        <td><%= event.description %></td>
        <td><%= event.address %></td>
        <td><%= event.additionalinfo %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

